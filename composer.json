{
    "name": "brightfish/blue-canary",
    "type": "project",
    "description": "Laravel-based monitoring and metrics collection server",
    "keywords": [
        "laravel",
        "monitoring",
        "metrics"
    ],
    "license": "GPL-3.0",
    "require": {
        "php": "^7.3",
        "ext-json": "*",
        "ext-pdo": "*",
        "fideloper/proxy": "^4.0",
        "laravel/framework": "^6.2",
        "laravel/tinker": "^1.0",
        "silber/bouncer": "v1.0.0-rc.6",
        "spatie/laravel-tags": "^2.5",
        "tymon/jwt-auth": "1.0.0-rc.5"
    },
    "require-dev": {
        "facade/ignition": "^1.4",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^3.0",
        "phpunit/phpunit": "^8.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "test": "vendor/bin/phpunit tests/Unit",
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ]
    }
}
